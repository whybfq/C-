int main() {
    cout << "please input your birthday year/month/day(8 numbers, each number one line or with space: \n"
            "for example: you can input 1 9 9 6 0 2 2 8";
    // prepare to improve the input, can make a excel with many inputs
    int a[8] = {0}, n[10] = {0}, sum = 0; //a[8]为输入的8个数，从0开始，n[8]为对应数字的圈圈个数
    for (int i = 0; i < 8; ++i) {
        cin >> a[i];
        sum += a[i];
        for (int j = 0; j <= 9; j++) //分别进行0~9之间的验证
            if (a[i] == j)n[j]++;
    }
    int t1, t2, t3, t4 = 0; // talent number and destiny number
    t1 = sum / 10;  //t1,t2为1~9之间的天赋数
    t2 = sum % 10;
    int m = t1 + t2;   //m为命运数 ，也必须在1~9之间
    if (m > 10) {  //若是命运数大于10，重新加
        t3 = m / 10;//t3,t4也为1~9之间的天赋数
        t4 = m % 10;
        m = t3 + t4;
        cout << "This person's tianfushu are: " << t1 << " " << t2 << " " << t3 << " " << t4 << " ";
    }//现在命运数为m,天赋数为t1,t2,t3,t4,先对天赋数含义进行分析
    else cout << "This person's tianfushu are: " << t1 << " " << t2 << " ";
    cout << "This person's mingyunshu is: " << m << endl;
    for (int j = 0; j <= 9; j++) {
        if (m == j)n[j] += 3;
        if (t1 == j)n[j] += 2;
        if (t2 == j)n[j] += 2;
        if (t3 == j && t3)n[j] += 2;
        if(t4==j && t4)n[j]+=2;
    }
    for(int i=0; i<10; i++) {
        cout<<i<<":now it has "<<n[i]<<" circles"<<endl;//so刚开始数组如果没有初始化会输出地址
    }
    int c,d,c1,d1;
    for(int i=0; i<=9; i++) {
        if(t1==i)c=i;
        if(t2==i)d=i;
        if(t3==i)c1=i;
        if(t4==i)d1=i;
    }

    switch (c) {   //jude the first tianfushu talent number
        case 0:
            break;
