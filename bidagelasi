    if(n[7] && n[8] && n[9]) {
        b[2]=n[7]+n[8]+n[9] ;//b[2]代表贵人
        cout << " 789连线——代表@贵人：追求无上的权利一生中会遇到很多贵人人缘非常好; \n";
        if(n[5]==0)
            cout << "表格中没有5时，任何事情都需要别人帮助，懒惰。\n";
    }
    if(n[1] && n[4] && n[7]) {
        b[3]=n[1]+n[4]+n[7];//b[3]代表物质
        cout << " 147连线代表@物质：爱情追求财富追求稳定的生活 \n";
        if(n[9]==0)
            cout << " 还有@贪婪过于现实铜臭拜金主义,表格中没有9。\n";
    }
    if(n[2] && n[5] && n[8]) {
        b[4]=n[2]+n[5]+n[8];//b[4]代表感情
        cout << " 258连线代表@感情:感情丰富，喜欢交朋友，对朋友重情义文笔不错, \n";
        if(n[7]==0)
            cout << " 也代表@饶舌:表格没有七，话多，喜欢和别人争辩。\n";
    }
    if(n[3] && n[6] && n[9]) {
        b[5]=n[3]+n[6]+n[9] ;//b[5]代表智慧
        cout << "369连线——代表@智慧:聪明智商高善于思考, \n";
        if(n[5]==0)
            cout << " 没有5的时候,代表@空想:爱做白日梦空想家。\n";
    }
    if(n[1] && n[5] && n[9]) {
        b[6]=n[1]+n[5]+n[9];//b[6]代表事业
        cout << " 159连线——代表@事业：企图心强事业心强，工作狂很容易变成工作狂，大女子主义者或大男子主义者，以工作为中心，忽略身边的人。\n";
    }
    if(n[3] && n[5] && n[7]) {
        b[7]=n[3]+n[5]+n[7] ;//b[7]代表人缘
        cout << " 357连线——代表@人缘：沟通能力强，受人欢迎，@八卦：好奇心过强，喜欢议论他人是非，偶尔有些讨厌。\n";
    }
    if(n[2] && n[4]) {
        b[8]=n[2]+n[4] ;//b[8]代表灵巧
        cout << " 24连线——代表@灵巧：待人接物做事，说话灵巧为人伶俐，也代表@谋略：狡猾地，有心计。\n";
    }
    if(n[2] && n[6]) {
        b[9]=n[2]+n[6] ;//b[9]代表平等
        cout << " 26连线——代表@平等：待人平等互动性强，善于借力；\n";
        if(n[9]==0)
            cout << " ，没有9——代表@利用：会利用别人 \n";
    }
    if(n[6] && n[8]) {
        b[10]=n[6]+n[8] ;//b[10]代表亲切
        cout << " 68连线——代表@亲切：待人亲切，诚实；\n";
        if(n[5]==0)
            cout << " 没有5代表：@隐瞒: 常常隐瞒自己的内心感受,即使遇到痛心的事也会强装欢笑。\n";
    }
    if(n[4] && n[8]) {
        b[11] = n[4] + n[8];//b[11]代表模范
        cout << " 48连线——代表: @模范:工作，努力负责; \n";
        if (n[5] == 0)
            cout << " 没有5——代表: 没有@安全感，常常没有安全感，不相信任何人。\n";
    }
    int cun = 0, bucun = 0; //统计存在与不存在的线的数量
    for (int i = 0; i < 12; i++) {
        if (b[i] == 0)bucun++;
        else cun++;
    } //12组连线都不存在
    if (bucun == 0)cout << " He/She 执行力强,做事雷厉风行,说到做到。\n";
    if (cun >= 4)cout << " He/She 做事很不专心,三心二意。\n";
    if (b[0] && b[5])cout << " He/She is very 感性 \n ";
    if (b[2] && b[3])cout << " He/She is very 理性 \n";
    return 0;
}


/*农历六月农历六个月出生的孩子心思细腻，虽然聪慧不足，但性格上的忍耐力相当过人。少年之时运势就会开启，这时候他的福气也会给家里带来财富，成年之后事业发展上有很多绝佳的好机会，财务收入也有源源不绝的趋势。
农历九月农历九月出生的孩子智慧锐敏，招四方之财。个性优雅迷人，为人忠诚，是个比较感性的人。但是不喜欢做决定，待人诚恳，讲求信用，喜欢接近年长的人，脑子里也常有些稀奇古怪的想法，甚至常有惊人之举。但是他天生就会给家里带来好运，长大成人之后也会大富大贵。
农历十月农历十月出生的孩子性情温和，重情重义，待人谦虚有礼，总希望做得尽善尽美，面面俱到，生活上有些小洁癖，不喜欢人家动你的东西。工作上有冲劲，能善尽职守。脑子反应快，喜欢追根究底，好奇心强，因为重视和谐，怕招惹人，经常把话藏在心里。但是却是天生好运的孩子，不仅自己福气漫天，也会带给家里。
