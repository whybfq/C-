#include <iostream>

using namespace std;

int main(){
    int q[100]={0,3,45,232,5,13,23,32},head=1,tail=10,i;   //初始化队列，假设队列中有9个数
    while(head < tail){
        cout<<q[head]<<" "<<endl;   //队首出队，打印队首
        ++head;

        q[tail] = q[head];   //将新队首队数添加到队尾
        ++tail;    //队尾加1，tail指向最后一个元素后一位
        ++head;    //再将队首出队
    }
}
