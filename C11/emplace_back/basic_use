#include <iostream>
#include <cstdio>
#include <cstring>
#include <cstdlib>
#include <fstream>
#include <functional>
#include <set>
#include <string>
#include <cmath>
#include <map>
#include <typeinfo>
#include <vector>
using namespace std;

struct A {
    int x;
    double y;
    A(int a, double b) : x(a), y(b) { }
};

int main(int argc, char const *argv[]) {
    vector<A> v;
    v.emplace_back(1,2);
    cout << v.size() << endl;  // 1
}
